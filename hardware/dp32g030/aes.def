[AES]
@ = 0x400BD000, 0x800

CR = 0x0000
> CCFC, 7, 1
= SET, 1

> CHMOD, 5, 2
= ECB, 0
= CBC, 1
= CTR, 2

> EN, 0, 1
= DISABLE, 0
= ENABLE, 1

SR = 0x0004
> CCF, 0, 1
= NOT_COMPLETE, 0
= COMPLETE, 1

DINR = 0x0008
DOUTR = 0x000C

KEYR0 = 0x0010
KEYR1 = 0x0014
KEYR2 = 0x0018
KEYR3 = 0x001C

IVR0 = 0x0020
IVR1 = 0x0024
IVR2 = 0x0028
IVR3 = 0x002C

